{
  "server_type": "video",
  "device": "tests/media/video.mp4",
  "on_data": [
    {
      "enabled": true,
      "name": "ext_frameb64",
      "path": "ext/video/frameb64.py",
      "config": {}
    },
    {
      "enabled": true,
      "name": "ext_yolo",
      "path": "ext/video/yolo.py",
      "config": {
        "weights": "res/yolo11n.pt",
        "cpu": false,
        "min_conf": 0.5,
        "img_size": [
          640,
          364
        ],
        "stream": true,
        "verbose": false,
        "fp16": true,
        "max_detect": 30,
        "vid_stride": 1
      }
    },
    {
      "enabled": true,
      "name": "ext_yunet",
      "path": "ext/video/yunet.py",
      "config": {
        "weights": "res/face_detection_yunet_2023mar_int8bq.onnx",
        "cpu": false,
        "img_size": [
          640,
          360
        ],
        "min_detect": 0.7,
        "max_nms": 0.3,
        "top_k": 5000
      }
    }
  ],
  "on_save": [],
  "socket": {
    "host": "127.0.0.1",
    "port": 9002
  },
  "log": {
    "logLevel": "NOTSET"
  }
}
